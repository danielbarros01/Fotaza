    "dotenv": "^16.3.1",
    "dropzone": "^5.9.3",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-validator": "^7.0.1",
    "imagesloaded": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "mercadopago": "1.5.16",
